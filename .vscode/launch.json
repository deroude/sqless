    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Start",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\core\\src\\index.ts",
            "args": ["start","-c","core/.sqless/sqless-config.yaml"],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Init",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\core\\src\\index.ts",
            "args": ["init","-a","examples/cms/cms-crud-api.yaml"],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}